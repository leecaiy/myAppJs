{
    "name": "加载jquery",
    "description" : "默认浏览器支持jquery",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "jquery.png"
     },

     "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["/lib/jquery-3.5.1.min.js","test.js"]
        }
      ]
    
  }

 